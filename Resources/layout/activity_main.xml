<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:text="Paul Ronald P. Bermudez"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/btnBermudez" 
        android:layout_centerHorizontal="true"/>
    <Button
        android:text="Raynielle Joseph Z. Borbe"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/btnBorbe" 
        android:layout_centerHorizontal="true"
        android:layout_below="@id/btnBermudez"/>
    <Button
        android:text="Louise Angela Caballero"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/btnCaballero" 
        android:layout_centerHorizontal="true"
        android:layout_below="@id/btnBorbe"/>
    <Button
        android:text="Ma. Angelica Lynneth J. Dano"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/btnDano"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/btnCaballero"/>
    <Button
        android:text="Zhen Marie G. Villanueva"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:minWidth="25px"
        android:minHeight="25px"
        android:id="@+id/btnVillanueva" 
        android:layout_centerHorizontal="true"
        android:layout_below="@id/btnDano"/>

</RelativeLayout>